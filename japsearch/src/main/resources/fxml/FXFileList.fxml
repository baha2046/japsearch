<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="800.0" prefWidth="480.0"
            xmlns="http://javafx.com/javafx/8.0.1" fx:controller="org.nagoya.controller.FXFileListControl">
    <Pane prefHeight="800.0" prefWidth="480.0" style="-fx-background-color: #363948;">
        <JFXButton fx:id="btnBrowse" contentDisplay="CENTER" layoutX="12.0" layoutY="37.0" maxHeight="40.0"
                   maxWidth="88.0" minHeight="40.0" minWidth="88.0" onAction="#browseAction" prefHeight="40.0"
                   prefWidth="88.0" ripplerFill="#f88b96" styleClass="custom-button"
                   stylesheets="@../css/fileListButton.css" text="Browse" textFill="#e8f0fa">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXButton>
        <JFXButton fx:id="btnUp" contentDisplay="CENTER" layoutX="100.0" layoutY="37.0" maxHeight="40.0"
                   maxWidth="88.0" minHeight="40.0" minWidth="88.0" onAction="#upDirectoryAction"
                   prefHeight="40.0" prefWidth="88.0" ripplerFill="#f88b96" styleClass="custom-button"
                   stylesheets="@../css/fileListButton.css" text="Up" textFill="#e8f0fa">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXButton>
        <JFXButton fx:id="btnReload" contentDisplay="CENTER" layoutX="276.0" layoutY="37.0" maxHeight="40.0"
                   maxWidth="88.0" minHeight="40.0" minWidth="88.0" onAction="#reloadAction" prefHeight="40.0"
                   prefWidth="88.0" ripplerFill="#f88b96" styleClass="custom-button"
                   stylesheets="@../css/fileListButton.css" text="Reload" textFill="#e8f0fa">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXButton>
        <JFXButton fx:id="btnRename" contentDisplay="CENTER" layoutX="12.0" layoutY="104.0"
                   onAction="#renameAction" ripplerFill="#f88b96" styleClass="custom-button"
                   stylesheets="@../css/fileListButton.css" text="Rename a single file" textFill="#e8f0fa">
            <font>
                <Font size="14.0"/>
            </font>
        </JFXButton>
        <JFXButton fx:id="btnDel" contentDisplay="CENTER" layoutX="364.0" layoutY="37.0" maxHeight="40.0"
                   maxWidth="88.0" minHeight="40.0" minWidth="88.0" onAction="#delAction" prefHeight="40.0"
                   prefWidth="88.0" ripplerFill="#f88b96" styleClass="custom-button"
                   stylesheets="@../css/fileListButton.css" text="Delete" textFill="#e8f0fa">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXButton>
        <JFXButton fx:id="btnSort" contentDisplay="CENTER" layoutX="188.0" layoutY="37.0" maxHeight="40.0"
                   maxWidth="88.0" minHeight="40.0" minWidth="88.0" onAction="#sortAction" prefHeight="40.0"
                   prefWidth="88.0" ripplerFill="#f88b96" styleClass="custom-button"
                   stylesheets="@../css/fileListButton.css" text="Sort" textFill="#e8f0fa">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXButton>
        <JFXListView fx:id="listView" layoutY="180.0" prefHeight="620.0" prefWidth="479.0"
                     style="-fx-background-color: #363948;" stylesheets="@../css/fileListButton.css"/>
        <JFXButton fx:id="btnRenameAll" contentDisplay="CENTER" layoutX="13.0" layoutY="138.0"
                   onAction="#renameAllAction" ripplerFill="#f88b96" styleClass="custom-button"
                   stylesheets="@../css/fileListButton.css" text="Rename all file in a directory"
                   textFill="#e8f0fa">
            <font>
                <Font size="13.0"/>
            </font>
        </JFXButton>
        <Label layoutX="14.0" layoutY="15.0" text="Directory" textFill="WHITE"/>
        <JFXButton fx:id="btnMove" contentDisplay="CENTER" layoutX="198.0" layoutY="138.0"
                   onAction="#moveAction" ripplerFill="#f88b96" styleClass="custom-button"
                   stylesheets="@../css/fileListButton.css" text="Move directory" textFill="#e8f0fa">
            <font>
                <Font size="13.0"/>
            </font>
        </JFXButton>
        <JFXButton contentDisplay="CENTER" layoutX="158.0" layoutY="104.0" onAction="#packDirectoryAction"
                   ripplerFill="#f88b96" styleClass="custom-button" stylesheets="@../css/fileListButton.css"
                   text="Pack a file into a directory" textFill="#e8f0fa">
            <font>
                <Font size="14.0"/>
            </font>
        </JFXButton>
    </Pane>
    <Label layoutX="14.0" layoutY="85.0" text="Rename Unity" textFill="WHITE"/>
</AnchorPane>
